<html t:type="Layout" title="twitterlib Index"
	t:sidebarTitle="Framework Version"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">
<!-- Most of the page content, including <head>, <body>, etc. tags, comes from Layout.tml -->

<head>
    <link rel="stylesheet" type="text/css" href="${context:/layout/RecommendedTweetTestPage.css}"/>
    <link rel="stylesheet" type="text/css" href="${context:/layout/Queue.css}"/>
   <!--   <link rel="stylesheet" type="text/css" href="${context:/layout/font-awesome.css}"/> -->
    <link rel="stylesheet" type="text/css" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" />
</head>

<h1>Queue Manager Demo</h1>

<fieldset class="sixb">

	<legend class="sixb">Content</legend>
	
	<table id="contentTable">

		<tr>
		
			<td class="messages">
				<div id="contentMessages">
					<br />	
					<div class="label label-important"><span class="mycenter">Action</span></div>				
				</div>
			</td>
			
			<td class="messages">
				<div id="queueMessages">
					<br />	
					<div class="label label-info message">
						<span class="mycenter">Queue</span>
						<span style="position: relative; top: -25px; right:-70px;">
							<div t:type="twitterlib/Queue" queueId="queueId" />
						</span>
					</div>
				</div>
			</td>
			
		</tr>
	
	</table>
	
</fieldset>

</html>